        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
    <style name="progressBarBlue" parent="@style/Theme.AppCompat">
        <item name="colorAccent">#2EFD00</item>
    </style>
